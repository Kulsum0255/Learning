   "source": [
    "fibo=[] # Empty list to store series\n",
    "n=int(input('Enter the number upto which you want the Fibonacci series'))\n",
    "for i in range(0,n+1):\n",
    "    if i==0 or i==1:\n",
    "        fibo.append(1)\n",
    "    else:\n",
    "        fibo.append(fibo[i-1]+fibo[i-2])\n",
    "print(fibo)"
   ]
